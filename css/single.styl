body
        min-width: 800px
        color: #333333

/* CONTENT PAGES STUFF */

h1
    text-align: center
    text-transform: uppercase

#nav
    text-align: center;
    position: fixed;
    top: 0px;
    margin: auto;
    z-index: 100000;
    width: 100%;
    background-color: #333333;
    color: white;


#nav ul
    display: inline;


#nav li
    display: inline-block;
    list-style-type:none;
    font-size: 1em;
    font-family: Tahoma;
    letter-spacing: .1em;
    text-align: center;
    width: 12em;
    font-weight: bold;
    padding: 20px 0;
    color: white;


#nav a
    text-decoration: none;
    color: white;


#nav a:hover
    color: #bbbbbb;

#content
    text-align: left;
    padding-top: 75px;
    width: 75%;
    min-width: 800px;
    margin: auto;


#navTitle
    font-family: 'Crimson Text', Georgia, serif;
    letter-spacing: .2em;
    font-weight: bold;
    display: inline-block;
    font-size: 2em;
    padding: 20px 0;


    /*Horizontal rules*/
#home
    .titleLetter
            cursor: pointer
            cursor: hand

    .doubleUneven
        width: 100px;
        border-top: solid #333333 3px;
        border-bottom: solid #333333 1px;
        padding: 1px;
        border-left: 0;
        border-right: 0;

    .contentp
        padding-left: 100px;

    .centered
        text-align: center;

    /* i styles */

    body.i-style
        font-family: 'Crimson Text', Georgia, serif;
        color: #333333;
        text-align: center;

    .i-style

        #logo
            margin: auto;
            padding-top: 12%;

        .titleLetter
            display: inline-block;
            width: 1em;
            font-size: 100px;
            font-weight: bold;

        #semicolon
            width: .3em;

        #subtitle
            font-size: 1.25em;
            letter-spacing: .3em;
            display: block;
            margin: auto;
            padding-top: 20px;
            text-transform: uppercase;

        #homeNav
            width: 100%;

        #homeNav a
            text-decoration: none;
            color: black;

        #homeNav a:hover
            color: #bbbbbb;

        #homeNav ul
            list-style-type:none;
            padding-top: 3%;
            font-size: 1em;
            font-family: Tahoma, sans-serif;
            letter-spacing: .2em;
            padding: 5% 0;
            margin: 0;

        #homeNav li
            display: inline-block;
            width: 14em;

        #homeNav li:hover
            color: #bbbbbb;
            font-family: Georgia;

        #homeRule
            width: 10%;
            display: block;
            margin-top: 5%;

        #catCredit
            display: none;

        #preload
            background: url("../img/bookcat.jpg") no-repeat -9999px -9999px;

    /* v styles */

    body.v-style
        background-color: black;
        color: #66ff33;
        font-family: 'VT323','Courier New', Courier, monospace;

    .v-style
        #logo
            margin: auto;
            text-align: center;
            padding-top: 12%;

        .titleLetter
            display: inline-block;
            font-size: 100px;
            text-transform: lowercase;

        #subtitle
            font-size: 1.25em;
            letter-spacing: .3em;
            text-align: center;
            display: block;
            margin: auto;
            padding-top: 20px;
            text-transform: uppercase;

        #homeNav
            text-align: center;
            width: 100%;

        #homeNav a
            text-decoration: none;
            color: #66ff33;

        #homeNav a:hover
            color: #cc33ff;

        #homeNav ul
            list-style-type:none;
            padding: 3% 0 0 0;
            font-size: 1em;
            letter-spacing: .2em;
            margin: 0;

        #homeNav li
            display: inline-block;
            width: 15em;
            height: 2.5em;
            margin: 3em;
            padding-top: 1em;

        #homeNav li:hover

        #dl
            border-top: 1px black solid;
            border-bottom: 1px black solid;

        .hide
            display: none;

        #catCredit
            display: none;

    /* a styles */

    body.a-style
        min-height: 100%;
        font-family: Impact,Haettenschweiler,"Franklin Gothic Bold",Charcoal,"Helvetica Inserat","Bitstream Vera Sans Bold","Arial Black","sans serif";
        font-weight: bold;
        color: white;
        background-image: url("../img/bookcat.jpg");
        background-repeat: no-repeat;
        background-size: cover;
        min-height: 100%;
        background-color: black;
        text-transform: uppercase;
        overflow: hidden;

    .a-style

        #logo
            margin: auto;
            text-align: center;

        /*text stroke lacks x-browser compatibility*/
        .titleLetter
            display: inline-block;
            font-size: 120px;
            -webkit-text-stroke-width: 3px;
           -webkit-text-stroke-color: black;

        #subtitle
            font-size: 100px;
            text-align: center;
            width: 100%;
            min-width: 800px;
            display: block;
            margin: auto;
            position: absolute;
            bottom: 0;
            -webkit-text-stroke-width: 3px;
            -webkit-text-stroke-color: black;
            z-index: -1;

        #homeNav
            text-align: center;
            font-size: 40px;
            width: 100%;

        #homeNav a
            text-decoration: none;
            color: white;
            font-weight: normal;
            background-color: black;
            width: 5em;
            height:  1.25em;
            display: inline-block;

        #homeNav a:hover
            color: blue;

        #homeNav ul
            list-style-type:none;
            padding: 0;
            margin: 0;

        #homeNav li
            display: inline-block;

        #homeNav li:hover

        #catCredit
            position: absolute;
            top: 0;
            right: 0;
            color: white;
            font-size: 12px;
            font-family: sans-serif;
            text-transform: none;

        #catCredit a
            text-decoration: none;
            color: white;

        .hide
            display: none;

    /* n styles */

    body.n-style
        background-color: #ff66ff;
        border-top: black 10px solid;
        border-left: black 10px solid;
        height: 98.5%;
        font-family: 'Franklin Gothic Medium', 'Franklin Gothic', 'ITC Franklin Gothic', Arial, sans-serif;
        font-weight: bold;
        color: black;
        text-transform: uppercase;

    .n-style
        /* typography -
        Calibri, Segoe UI, Arial, sans-serif
        */
        /*rotation needs x-browser compatibility for IE*/

        #logo
            -webkit-transform: rotate(-45deg);
            -moz-transform: rotate(-45deg);
            -ms-transform: rotate(-45deg);
            -o-transform: rotate(-45deg);
            display: block;
            float: left;
            margin-top: 200px;
            width: 900px;

        .titleLetter
            display: inline-block;
            font-size: 180px;

        #subtitle
            font-family: Georgia, Times, "Times New Roman", serif;
            font-size: 50px;
            width: 50%;
            max-width: 800px;
            left:470px;
            position: relative;

        #homeNav
            font-size: 40px;
            letter-spacing: .1em;
            display: inline-block;
            float: left;
            width: 50%;
            margin-left: 300px;
            padding-top: .5em;

        #homeNav a
            text-decoration: none;
            display: inline-block;
            margin-right: 1em;
            color: black;

        #homeNav a:hover

        #homeNav ul
            list-style-type:none;
            padding: 0;
            margin: 0;

        #homeNav li
            display: inline-block;

        #homeNav li:hover

        .caps
            font-size: 1.3em;

        #catCredit
            display: none

        .hide
            display: none;

    /* h styles */

    body.h-style
        font-family: Arial, Helvetica Neue, Helvetica, sans-serif;
        background-color: black;
        font-size: 100px;
        color: white;
        padding-top: 5%;
        padding: 0;
        margin: 0;
        text-align: center;

    .h-style
        /* 60s/70s */

        #logo
            font-family: 'Fjalla One', sans-serif;
            margin: 10px;
            width: 7em;
            height:0;
            padding-bottom: 7em;
            -moz-border-radius: 7em;
            -webkit-border-radius: 7em;
            border-radius: 50%;
            background: #005C8A;
            display: inline-block;
            float: left;
            text-align: left;

        .titleLetter
            color:white;
            font-size: 2em;
            display: inline-block;
            font-weight: bold;
            line-height: 4.5em;
            margin-right: -.1em;

        #subtitle
            background: -webkit-linear-gradient(top, #ff9900, #B8E62E);
            background: linear-gradient(top, #ff9900, #B8E62E);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            display: block;
            float: left;
            width: 35%;
            padding-top: 2em;
            margin: 10px;
            text-align: left;

        #homeNav
            float: left;
            font-size: .5em;
            border-bottom: white solid 15px;
            border-top: white solid 15px;
            text-align: center;
            padding-left: 50px;
            padding-right: 50px;
            width: 30%;
            margin-left: 15em;
            position: absolute;

        #homeNav li
            display: inline-block;
            margin-right: .1em;
            text-align: center;

        #homeNav a
            text-decoration: none;
            color: #ff9900;

        #homeNav a:hover
            text-decoration: none;
            color: #B8E62E;

        .hide
            display: none;

        #catCredit
            display: none;

    /* o styles */

    body.o-style
        font-family: Arial, Helvetica Neue, Helvetica, sans-serif;
        background-color: cyan;
        background-image: url("../img/tile2.png");
        color: blue;
        padding: 3%;
        text-align: center;
        font-size: 75px;
        cursor: crosshair;

    .o-style
        /* Geocities style
        kinda want a custom pointer but it's okay for now
        */

        #logo
            background-color: white;
            background-image: url("../img/notfound.jpg");
            background-position: 98% 5%;
            background-repeat: no-repeat;
            display: block;
            margin: auto;
            border: red 1px solid;
            width: 9em;

        #i
            color: red;
            font-size: 100px;

        .titleLetter
            display: inline-block;
            font-weight: bold;
            font-size: 75px;
            color: fuchsia;

        #subtitle
            font-size: 50px;
            font-family: Papyrus, fantasy;

        #homeNav
            font-size: 20px;

        #homeNav a
            padding: 7px;
            border: inset gray 4px;
            background-color: gray;

        #homeNav ul
            font-size: 1em;
            font-family: Tahoma, sans-serif;

        #homeNav li
            display: inline-block;

        #homeRule
          width: 75%;
          color: none;
          display: block;
          border-top: dotted 10px red;

        #catCredit
            display: none;

        .hide
            display: none;

    /* e styles */

    body.e-style
        font-family: 'Cutive Mono', 'Courier New', Courier, monospace ;
        padding: 3em;

    .e-style
        /* typewriter */
        .hide
            display: none;

        #logo
            border-bottom: black solid 1px;
            display: inline-block;
            margin-bottom: 1em;

        a
           color: black;
           text-decoration: none;

        ul
            margin: 1em 0;
            padding: 0;

        li
          list-style: none;

        /*replace bullets with dashes - questionably x-browser compatible */
        ul li:before
            content: '-';
            margin: 0;    /* any design */

        #catCredit
            display: none;

/* header styles */
.v-style div#nav
    background-color: black

.v-style div#nav a
    color: #66ff33
    font-family: 'VT323','Courier New', Courier, monospace

